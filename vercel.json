{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "APP_ENV": "production",
    "SESSION_LIFETIME": "7200"
  }
}